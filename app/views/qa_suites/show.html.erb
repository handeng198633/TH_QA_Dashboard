<% provide(:title, 'Suites Detail') %>

          <div class="row">
            <div class="col-md-12">
              <div class="box">
                <div class="box-header">
                  <h3 class="box-title">Suite Detail</h3>
                </div><!-- /.box-header -->
                <div class="box-body no-padding">
                  <table class="table table-striped">
                    <tr>
                      <th style="width: 80px">Suite Name</th>
                      <th style="width: 40px">Branch</th>
                      <th style="width: 40px">Type</th>
                      <th style="width: 60px">Start Time</th>
                      <th style="width: 60px">End Time</th>
                      <th style="width: 40px">Status</th>
                      <th style="width: 20px">Passed</th>
                      <th style="width: 20px">Failed</th>
                    </tr>
                    <!--<%= render @qa_suites %>-->
                  </table>
                </div><!-- /.box-body -->
              </div><!-- /.box -->
            <%= will_paginate %> 
            </div><!-- /.col -->
          </div><!-- /.row -->

